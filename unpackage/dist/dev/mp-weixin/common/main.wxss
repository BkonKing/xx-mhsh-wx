page {
  width: 100%;
  min-height: 100%;
  background-color: #f2f2f4;
  font-size: 28rpx;
}
button, input, view, text, scroll-view {
  box-sizing: border-box;
  font-size: 28rpx;
}
image {
  width: initial;
  height: initial;
}
button {
  -webkit-tap-highlight-color: initial;
}
button::after {
  border: none;
}
navigator {
  display: inline-block;
}
.tf-bg-white {
  background-color: #fff;
}
.tf-body {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.tf-flex {
  display: flex;
}
.tf-flex-row {
  display: flex;
  flex-direction: row;
}
.tf-flex-col {
  display: flex;
  flex-direction: column;
}
.tf-flex-1 {
  flex: 1;
}
.tf-row-justify-center {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.tf-row-justify-between {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.tf-row-justify-end {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}
.tf-row-justify-around {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.tf-row-items-center {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tf-column-justify-around {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.tf-column-justify-center {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.tf-flex-center {
  display: flex;
  align-items: center;
  justify-content: center;
}
