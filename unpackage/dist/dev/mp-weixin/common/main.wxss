page {
  --tag-primary-color: #448fe4;
  --tag-warning-color: #ffc017;
  --tag-danger-color: #eb5841;
}
page .tf-uni-list {
  background-color: #fff;
  border-radius: 8rpx;
}
page .tf-uni-list  .uni-list {
  background-color: #fff;
  border-radius: 8rpx;
}
page .tf-uni-list  .uni-list--border {
  left: 30rpx;
  right: 30rpx;
}
page .tf-uni-list  .uni-list-item {
  background-color: initial;
}
page  .uni-list-item__extra-text {
  font-size: 28rpx;
  color: #000;
}
page  .uni-card {
  margin: 30rpx 20rpx 0 !important;
  padding: 0 30rpx !important;
}
page  .uni-card .uni-card__header,
page  .uni-card .uni-card__content {
  padding: 20rpx 0 !important;
}
page  .uni-navbar .uni-nav-bar-text {
  font-size: 32rpx;
}

